{
    "name": "My Pyramid App",
    "package_name": "{{ cookiecutter.name.lower().replace(' ', '_').replace('-', '_') }}",
    "slug": "{{ cookiecutter.package_name.replace('_', '-') }}",
    "short_description": null,
    "python_version": "3.10.4",
    "port": "{{ '0123456789'|random  }}{{ '0123456789'|random  }}{{ '0123456789'|random  }}{{ '0123456789'|random  }}",
    "github_owner": "hypothesis",
    "copyright_holder": "Hypothesis",
    "visibility": ["public", "private"],
    "create_github_repo": ["no", "yes"],
    "__github_url": "https://github.com/{{ cookiecutter.github_owner }}/{{ cookiecutter.slug }}",
    "__dependabot_interval": "weekly",
    "__docker": ["no", "yes"],
    "__docker_namespace": "{{ cookiecutter.github_owner }}",
    "__docker_network": "{{ cookiecutter.package_name }}_default",
    "__services": ["no", "yes"],
    "__db": ["no", "yes"],
    "__frontend": ["no", "yes"],
    "__frontend_typechecking": "{{ cookiecutter.__frontend }}",
    "_extensions": ["local_extensions.LocalJinja2Extension"],
    "_directory": "pyramid-app",
    "__ignore__": "",
    "__target_dir__": ""
}
